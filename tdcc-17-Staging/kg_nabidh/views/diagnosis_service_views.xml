<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="diagnosis_service_action" model="ir.actions.act_window">
        <field name="type">ir.actions.act_window</field>
        <field name="name">Diagnosis Service</field>
        <field name="view_mode">tree,form</field>
        <field name="res_model">diagnosis.service</field>

    </record>
    <record id="diagnosis_service_tree" model="ir.ui.view">
        <field name="name">diagnosis.service.tree.views</field>
        <field name="model">diagnosis.service</field>
        <field name="arch" type="xml">
            <tree>
                 <field name="code"/>
                <field name="name"/>


            </tree>
        </field>
    </record>
    <record id="diagnosis_service_form" model="ir.ui.view">
        <field name="name">diagnosis.service.form.views</field>
        <field name="model">diagnosis.service</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="code"/>
                        </group>
                        <group>
                            <field name="name"/>

                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <menuitem id="diagnosis_service_menu"
              action="diagnosis_service_action"
              name="Diagnosis Service"
              parent="kg_nabidh.obr_menu"/>

</odoo>
