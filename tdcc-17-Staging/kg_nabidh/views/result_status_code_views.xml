<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="result_status_code_action" model="ir.actions.act_window">
        <field name="type">ir.actions.act_window</field>
        <field name="name">Result Status Code</field>
        <field name="view_mode">tree,form</field>
        <field name="res_model">result.status.code</field>

    </record>
    <record id="result_status_code_tree" model="ir.ui.view">
        <field name="name">result.status.code.tree.views</field>
        <field name="model">result.status.code</field>
        <field name="arch" type="xml">
            <tree>
                 <field name="code"/>
                <field name="name"/>


            </tree>
        </field>
    </record>
    <record id="admision_type_form" model="ir.ui.view">
        <field name="name">result.status.code.form.views</field>
        <field name="model">result.status.code</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="code"/>
                        </group>
                        <group>
                            <field name="name"/>

                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <menuitem id="result_status_code_menu"
              action="result_status_code_action"
              name="Result Status Code"
              parent="kg_nabidh.obx_menu"/>

</odoo>
