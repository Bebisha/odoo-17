<odoo>
    <record id="vat_return_wizard_form" model="ir.ui.view">
        <field name="name">vat.return.report.form</field>
        <field name="model">vat.return.report</field>
        <field name="priority" eval="8"/>
        <field name="arch" type="xml">
            <form string="Vat Return Report">
                <group>
                    <group>
                        <field name="date_from" required="1"/>
                        <field name="target_moves" widget="radio"/>

                    </group>
                    <group>
                        <field name="date_to" required="1"/>
                        <field name="company_id"/>
                    </group>
                    <group>
                        <field name="fileout" invisible="1"/>
                        <field name="fileout_filename" invisible="1"/>
                    </group>
                </group>
                <footer>
                    <button string="Print xlsx" name="xlsx_print" type="object" class="btn-primary"/>
                    <button string="Cancel" name="cancel" type="object" special="cancel" class="btn-cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>