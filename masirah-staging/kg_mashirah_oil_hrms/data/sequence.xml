<odoo>
    <data>
        <record id="employee_training_for_seq" model="ir.sequence">
            <field name="name">Employee Training Sequence</field>
            <field name="code">emp.training.seq</field>
            <field name="prefix">EMT</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="annual_transfer_for_seq" model="ir.sequence">
            <field name="name">Annual Transfer Sequence</field>
            <field name="code">annual.transfer.seq</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="leave_encashment_for_seq" model="ir.sequence">
            <field name="name">Leave Encashment Sequence</field>
            <field name="code">leave.encashment.seq</field>
            <field name="prefix">LE</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="employee_certificate_seq" model="ir.sequence">
            <field name="name">Employee Certificate Sequence</field>
            <field name="code">employee.certificate.seq</field>
            <field name="prefix">C/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="job_grade_seq" model="ir.sequence">
            <field name="name">Job Grade Sequence</field>
            <field name="code">job.grade.seq</field>
            <field name="prefix">G</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>