<odoo>
    <data>
        <record id="kg_inherit_manufacturing_configuration" model="ir.ui.view">
            <field name="name">Manufacturing Inherit Config</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//app[@name='stock']" position="inside">
                    <h2>Material Consumption</h2>
                    <div class="row mt16 o_settings_container" name="booking_setting_container">
                        <div class="o_setting_right_pane">
                            <div class="content-group">
                                <div class="mt16">
                                    <label for="destination_location" string="Destination Location"
                                           class="col-3 col-lg-3 o_light_label"/>
                                    <field name="destination_location" class="oe_inline" domain="[('usage', '=', 'internal')]"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
