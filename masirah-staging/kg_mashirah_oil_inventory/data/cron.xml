<odoo>
    <data>
        <record id="ir_cron_change_expire_state" model="ir.cron">
            <field name="name">Material Consumption: Change state</field>
            <field name="model_id" ref="model_material_consumption"/>
            <field name="state">code</field>
            <field name="code">model.update_state()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>
        <record id="ir_cron_update_product_code" model="ir.cron">
            <field name="name">Product: Update Product Code</field>
            <field name="model_id" ref="model_product_template"/>
            <field name="state">code</field>
            <field name="code">model.kg_update_product_code()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>
        <record id="ir_cron_create_category_sequence" model="ir.cron">
            <field name="name">Product Category: Create Sequence</field>
            <field name="model_id" ref="model_product_category"/>
            <field name="state">code</field>
            <field name="code">model.kg_create_sequence()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>
    </data>
</odoo>