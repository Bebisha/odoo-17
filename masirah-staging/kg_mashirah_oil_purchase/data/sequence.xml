<odoo>
    <data noupdate="0">

        <record id="kg_purchase_seq" model="ir.sequence">
            <field name="name">Purchase Order Sequence</field>
            <field name="code">kg.purchase.seq</field>
            <field name="prefix">PO/%(year)s/%(month)s/</field>
            <field name="padding">3</field>
<!--            <field eval="True" name="use_date_range"/>-->
            <field name="company_id" eval="False"/>
        </record>

        <record id="kg_purchase_contract_seq" model="ir.sequence">
            <field name="name">Purchase Contract Sequence</field>
            <field name="code">kg.purchase.contract.seq</field>
            <field name="prefix">PC/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="kg_seq_rfq" model="ir.sequence">
            <field name="name">RFQ</field>
            <field name="code">kg.purchase.rfq</field>
            <field name="prefix">RFQ/%(year)s/%(month)s/</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
<!--            <field name="number_increment">1</field>-->
            <!--            <field name="use_date_range">True</field>-->
<!--            <field name="company_id" eval="False"/>-->
        </record>

    </data>
</odoo>