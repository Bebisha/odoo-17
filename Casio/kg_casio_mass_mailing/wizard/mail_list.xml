<odoo>
  <data>

      <record id="mail_listing_form" model="ir.ui.view">
            <field name="name">email.list.form</field>
            <field name="model">email.list</field>
            <field name="arch" type="xml">
                <form string="voucher" version="7.0" duplicate="0">
                    <sheet>
                        <group>

                        </group>
                        <field name="line_ids" >
                            <tree create="false" delete="false" editable="bottom" decoration-danger="is_removed == True">
                                <field name="partner_id" readonly="1" force_save="1"/>
                                <field name="email" readonly="1" force_save="1"/>
                                <field name="preference" readonly="1" force_save="1"/>
                                <field name="fully_unsubscribed" readonly="1" force_save="1"/>
                                <field name="total_email_sent" readonly="1" force_save="1"/>
                                <field name="is_removed" string="Remove"/>
                                <field name="object_id" invisible="1" readonly="1" force_save="1"/>
                            </tree>
                        </field>
                    </sheet>
                    <footer>
                    <button name="remove_from_mail_list" string="Remove from Email" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
                </form>
            </field>
        </record>

