                header { display: none !important; }
                footer { display: none !important; }

                .qr-social-row {
                display: flex;
                justify-content: center;
                align-items: center;
                gap: 40px;
                margin-top: 40px;
                white-space: nowrap;
                overflow-x: auto;
                flex-wrap: nowrap;
                }

                .qr-item {
                text-align: center;
