<odoo>

    <record id="seq_invoice_number" model="ir.sequence">
        <field name="name">KG Invoice Number Sequence</field>
        <field name="code">kg.invoice.no.seq</field>
        <field name="prefix">RAW-%(y)s-</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
        <field name="active" eval="False"/>
    </record>

</odoo>