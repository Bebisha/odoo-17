<odoo>

    <record id="view_stock_move_tree_inventory_inherit_d" model="ir.ui.view">
        <field name="name">stock.move.inventory.tree.inherit.id</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_uom']" position="after">
                <field name="kilograms" optional="show" sum="Total Kg"/>
            </xpath>
        </field>
    </record>

    <record id="kg_view_move_search_inherit" model="ir.ui.view">
        <field name="name">kg.stock.move.search.inherit.id</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_search"/>
        <field name="arch" type="xml">
            <xpath expr="//filter[@name='inventory']" position="after">
                <filter name="excluding_sale_location" string="Excluding Warehouse"
                        domain="[('is_non_sale_location','=',False),('state','=', 'done')]"/>
                <filter name="including_sale_location" string="Including Warehouse"
                        domain="[('state','=', 'done')]"/>
            </xpath>
        </field>
    </record>


</odoo>