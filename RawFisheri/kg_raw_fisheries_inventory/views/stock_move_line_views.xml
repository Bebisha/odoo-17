<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="view_stock_move_line_tree_inventory_inherit_d" model="ir.ui.view">
        <field name="name">stock.move.line.inventory.tree.inherit.id</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_move_line_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_uom_id']" position="after">
                <field name="kilograms" optional="show" sum="Total Kg"/>
            </xpath>
            <xpath expr="//field[@name='quantity']" position="attributes">
                <attribute name="sum">Total Qty</attribute>
            </xpath>
        </field>
    </record>

</odoo>