<odoo>
    <record id="kg_spreadsheet_dashboard_inventory_on_hand" model="spreadsheet.dashboard">
        <field name="name">Inventory On Hand</field>
        <field name="spreadsheet_binary_data" type="base64"
               file="kg_raw_fisheries_inventory/data/files/inventory_on_hand_dashboard.json"/>
        <field name="dashboard_group_id" ref="spreadsheet_dashboard.spreadsheet_dashboard_group_logistics"/>
        <field name="group_ids" eval="[Command.link(ref('stock.group_stock_manager'))]"/>
        <field name="sequence">300</field>
    </record>

    <record id="spreadsheet_dashboard_stock_account.spreadsheet_dashboard_inventory_on_hand"
            model="spreadsheet.dashboard">
        <field name="group_ids" eval="[(5, 0, 0)]"/>
    </record>

    <record id="kg_spreadsheet_dashboard_inventory_flow" model="spreadsheet.dashboard">
        <field name="name">Inventory Flow</field>
        <field name="spreadsheet_binary_data" type="base64"
               file="kg_raw_fisheries_inventory/data/files/inventory_flow_dashboard.json"/>
        <field name="dashboard_group_id" ref="spreadsheet_dashboard.spreadsheet_dashboard_group_logistics"/>
        <field name="group_ids" eval="[Command.link(ref('stock.group_stock_manager'))]"/>
        <field name="sequence">400</field>
    </record>

    <record id="spreadsheet_dashboard_stock.spreadsheet_dashboard_inventory_flow" model="spreadsheet.dashboard">
        <field name="group_ids" eval="[(5, 0, 0)]"/>
    </record>

</odoo>
