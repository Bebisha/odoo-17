            <field name="code">EXIN</field>
            <field name="parent_id" ref="hr_payroll.ALW"/>
        </record>

        <record id="DISCHARGE" model="hr.salary.rule.category">
            <field name="name">Discharge</field>
            <field name="code">DISCHARGE</field>
