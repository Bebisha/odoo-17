<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="crew_report_wizard_form" model="ir.ui.view">
            <field name="name">crew.salary.report.wizard.form</field>
            <field name="model">crew.salary.report.wizard</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Crew Salary Report">
                    <group>
                        <group>
                            <field name="start_date" required="1"/>
                            <field name="vessel_id"/>
                        </group>
                        <group>
                            <field name="end_date" required="1"/>
                        </group>
                    </group>
                    <footer>
                        <button string="VIEW" name="action_view_report" type="object" class="btn-primary"/>
                        <button string="Cancel" name="cancel" type="object" special="cancel" class="btn-cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>