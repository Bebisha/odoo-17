<odoo>

    <record id="kg_seq_purchase_enquiry" model="ir.sequence">
        <field name="name">Purchase Enquiry Sequence</field>
        <field name="code">po.enquiry.seq</field>
        <field name="prefix">PE</field>
        <field name="padding">5</field>
        <field name="implementation">no_gap</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_purchase_entry" model="ir.sequence">
        <field name="name">Purchase Entry Sequence</field>
        <field name="code">po.entry.seq</field>
        <field name="prefix">PO</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>


</odoo>