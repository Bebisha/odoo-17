<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="common_order_form" model="ir.ui.view">
        <field name="name">common.order.form</field>
        <field name="model">common.order</field>
        <field name="arch" type="xml">
            <form>
                <sheet>

                    <group>
                        <group>
                            <field name="full_name" invisible="1"/>
                             <field name="confidentiality_code" string="Operation codeSystem"/>
                            <field name="order_type" string="Medication"/>
                            <field name="order_control"/>
                            <field name="order_status"/>


                        </group>
                        <group>
                            <field name="ordering_provider" invisible="1"/>
                             <field name="date_transaction"/>
                            <field name="quantity" invisible="1"/>
                            <field name="interval" invisible="1"/>
                            <field name="duration" invisible="1"/>
                            <field name="priority" invisible="1"/>
                            <field name="text" invisible="1"/>
                            <field name="start_date"/>
                            <field name="end_date"/>

                            <field name="name" invisible="1"/>
                        </group>

                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>