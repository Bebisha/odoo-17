    <record id="client_intake_partner" model="ir.ui.view">
        <field name="name">Partner details</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <data>
                <xpath expr="//sheet" position="before">
                    <header>
                        <button name="action_appointment" string="Create Appointment" type="object"
                                class="oe_highlight" invisible="state != 'new'"/>
                        <button name="action_merge_patient" string="Merge" type="object"
                                invisible="state != 'new'"/>
                        <button name="action_update" string="Update" type="object"
                                invisible="1"/>
                        <button name="discharge_event" string="Discharge Event" type="object"
                                invisible="1"/>
                        <button name="action_update_a31" string="A31" type="object"
                                invisible="1"/>

                        <button name="action_vo4" string="V04" type="object"
                                invisible="1"/>
                        <button name="cancel_admit" string="Cancel Admit" type="object"
                                invisible="1"/>
                        <!--                        <button name="get_values_A39" string="Merge Patient" type="object" class="oe_highlight"  />-->
                        <button name="oder_message_O01" string="O01" type="object"
                                invisible="1"/>
                        <!--                        <button name="problem_record_pc1" string="PC1" type="object"-->
                        <!--                                invisible="state != 'new'"/>-->
                        <button name="original_document_notification" string="T02" type="object" invisible="1"/>
                        <button name="edit_notification" string="T08" type="object" invisible="1"/>
                        <button name="unsolicited_transmission_observation" string="R01" type="object" invisible="1"/>

                        <field name="state" widget="statusbar" invisible="1"/>
                        <!--                        <widget name="web_ribbon" title="Discharge"-->
                        <!--                                invisible="state != 'dischager'  " />-->
                        <widget name="web_ribbon" title="Discharge" bg_color="text-bg-danger"
                                invisible="state != 'dischager'"/>

                    </header>
                </xpath>
                <xpath expr="//button[@name='action_view_opportunity']" position="before">
                    <button name="action_appoinment" type="object" class="oe_stat_button" icon="fa-id-card-o"
                            string="Appointment"/>
                    <button name="action_merge" type="object" class="oe_stat_button" icon="fa-id-card-o"
                            string="Merge" invisible="1"/>
                </xpath>
                <xpath expr="//field[@name='mobile']" position="attributes">
                    <attribute name="required">is_physician == True</attribute>
                </xpath>

            </data>
        </field>
    </record>

    <record id="physion_res_partner_inherit" model="ir.ui.view">
        <field name="name">Partner details</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="bista_tdcc_operations.res_partner_field_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='physician_code_id']" position="before">
                <field name="sheryan_id" invisible="is_physician !=  True"/>
            </xpath>
        </field>
    </record>
</odoo>