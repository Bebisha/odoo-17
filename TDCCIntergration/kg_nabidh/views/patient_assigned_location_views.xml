<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="patient_assigned_location_action" model="ir.actions.act_window">
        <field name="type">ir.actions.act_window</field>
        <field name="name">Patient Assigned Location</field>
        <field name="view_mode">tree,form</field>
        <field name="res_model">patient.assigned.location</field>

    </record>
    <record id="patient_assigned_location_tree" model="ir.ui.view">
        <field name="name">patient.assigned.location.views</field>
        <field name="model">patient.assigned.location</field>
        <field name="arch" type="xml">
            <tree>
                 <field name="code"/>
                <field name="name"/>


            </tree>
        </field>
    </record>
    <record id="patient_assigned_location_form" model="ir.ui.view">
        <field name="name">patient.assigned.location.form.views</field>
        <field name="model">patient.assigned.location</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="code"/>
                        </group>
                        <group>
                            <field name="name"/>

                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <menuitem id="patient_assigned_location_menu"
              action="patient_assigned_location_action"
              name="Patient Assigned Location"
              parent="kg_nabidh.pid_menu"/>



</odoo>