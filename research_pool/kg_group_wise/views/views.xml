<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="kg_branch_tree">
      <field name="name">kg_branch tree</field>
      <field name="model">kg_branch</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="name" required="1"/>
          <field name="company_id" readonly="1"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="kg_branch_action_window">
      <field name="name">Branches</field>
      <field name="res_model">kg_branch</field>
      <field name="view_mode">tree</field>
    </record>

        <menuitem name="Branches" id="kg_branch_menu" parent="base.menu_users"
              action="kg_branch_action_window"/>

