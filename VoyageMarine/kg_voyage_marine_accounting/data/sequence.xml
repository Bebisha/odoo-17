<odoo>
    <data>

        <record id="seq_employee_expense" model="ir.sequence">
            <field name="name">Employee Expense</field>
            <field name="code">employee.expense</field>
            <field name="prefix">EME/%(y)s%(month)s/</field>
            <field name="number_increment" eval="1"/>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>