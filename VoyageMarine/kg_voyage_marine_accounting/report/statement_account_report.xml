                    <t t-foreach="values" t-as="vals" t-foreach-index="p_idx">
                        <table style="width:100%;font-size:14px;padding-left:10px;" class="table-borderless">
                            <tr width="100%">
                                <div style="page-break-before: always;"/>
                                <td width="100%" colspan="4" align="center"
                                    style="font-size:25px;padding-right:50px;padding-bottom:15px;padding-top:15px;">
                                    <strong>Statement of Account</strong>
<!--                                    <span style="float:right;font-size:12px;">-->
<!--                                        Page-->
<!--                                        <t t-esc="(p_idx or 0) + 1"/>-->
<!--                                        .1-->
<!--                                    </span>-->
                                </td>
                            </tr>
                            <tr width="100%">
                                <td width="5%" style="align:left;padding-left:5px;">To:</td>
                                <td width="45%" style="align:left;padding-left:5px;">
                                    <strong>
                                        <span t-esc="vals['partner_name']"/>
                                    </strong>
                                </td>
                                <td width="50%" colspan="2" align="right" style="padding-right:50px;">
                                    <span>SOA as</span>
                                    <span t-esc="date_to" t-options="{'widget': 'date','format': 'dd/MM/yyyy'}"/>
                                </td>
                            </tr>
                            <tr width="100%">
                                <td width="5%" style="align:left;padding-left:5px;"/>
                                <td width="45%" style="align:left;padding-left:5px;">
                                    <t t-if="vals['partner_zip']">
                                        <span>PO Box:</span>
                                        <span t-esc="vals['partner_zip']"/>
                                        ,
                                    </t>
                                    <t t-if="vals['partner_state']">
                                        <span t-esc="vals['partner_state']"/>
                                        ,
                                    </t>
                                    <t t-if="vals['partner_country']">
                                        <span t-esc="vals['partner_country']"/>
                                    </t>
                                    <t t-if="vals['partner_phone']">
                                        <br/>
                                        <span>Tel:</span>
                                        <span t-esc="vals['partner_phone']"/>
                                    </t>
                                </td>
                                <td width="50%" colspan="2" align="right" style="padding-right:50px;"/>
                            </tr>
                            <tr width="100%">
                                <td width="5%" style="align:left;padding-left:5px;padding-top:10px;">Attn:</td>
                                <td width="45%" style="align:left;padding-left:5px;padding-top:10px;">
                                    <span>Account Department,</span>
                                </td>
                                <td width="50%" colspan="2" align="right" style="padding-right:50px;padding-top:10px;"/>
                            </tr>
                        </table>
                        <br/>
                        <table style="border-collapse:collapse;width:100%;margin:5pt;border-radius: 5px 5px 5px 5px;"
                               class="table-borderless">
                            <tr width="100%"
                                style="font-size:12px;border-top: 1pt solid black;border-bottom: 1pt solid black;">
                                <td align="center" width="12%"
                                    style="padding-left:5px;padding-right:5px;padding-top:5px;padding-bottom:5px;">
                                    <strong>Invoice Date</strong>
                                </td>
                                <td align="center" width="16%"
                                    style="padding-left:5px;padding-right:5px;padding-top:5px;padding-bottom:5px;">
                                    <strong>Invoice No</strong>
                                </td>
                                <td align="center" width="12%"
                                    style="padding-left:5px;padding-right:5px;padding-top:5px;padding-bottom:5px;">
                                    <strong>Ref/PO No</strong>
                                </td>
                                <td align="center" width="12%"
                                    style="padding-left:5px;padding-right:5px;padding-top:5px;padding-bottom:5px;">
                                    <strong>Vessel/Project</strong>
                                </td>
                                <td align="center" width="12%"
                                    style="padding-left:5px;padding-right:5px;padding-top:5px;padding-bottom:5px;">
                                    <strong>Invoice Amount</strong>
                                </td>
                                <td align="center" width="12%"
                                    style="padding-left:5px;padding-right:5px;padding-top:5px;padding-bottom:5px;">
                                    <t t-if="account_type == 'asset_receivable'">
                                        <strong>Received Amount</strong>
                                    </t>
                                    <t t-else="">
                                        <strong>Paid Amount</strong>
                                    </t>
                                </td>
                                <td align="center" width="12%"
