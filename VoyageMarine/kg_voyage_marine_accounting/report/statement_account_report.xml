<odoo>
    <template id="soa_report_template_id">
        <t t-call="web.html_container">
            <t t-call="web.basic_layout">
                <div class="page">
                    <div class="header">
                        <table class='table table-borderless table-condensed table-hover'>
                            <tr width="100%">
                                <td style="width: 50%;" align="left">
                                    <t t-if="company_logo">
                                        <img t-att-src="company_logo" height="100px" width="75%"/>
                                    </t>
                                </td>
                                <td style="width: 50%;font-size:14px;" align="right">
                                    <t t-if="company_name">
                                        <strong>
                                            <span t-esc="company_name"/>
                                        </strong>
                                        <br/>
                                    </t>
                                    <t t-if="company_phone">
                                        <span t-esc="company_phone"/>
                                        <i class="fa fa-phone" aria-hidden="true"
                                           style="margin-left: 5px; border-radius: 50%; background-color: #ddd; padding: 5px;"/>
                                        <br/>
                                    </t>
                                    <t t-if="company_mobile">
                                        <span t-esc="company_mobile"/>
                                        <i class="fa fa-fax" aria-hidden="true"
                                           style="margin-left: 5px; border-radius: 50%; background-color: #ddd; padding: 5px;"/>
                                        <br/>
                                    </t>
                                    <t t-if="company_email">
                                        <span t-esc="company_email"/>
                                        <i class="fa fa-envelope" aria-hidden="true"
                                           style="margin-left: 5px; border-radius: 50%; background-color: #ddd; padding: 5px;"/>
                                        <br/>
                                    </t>
                                    <t t-if="company_zip">
                                        <span>
                                            P.O. Box:
                                            <span t-esc="company_zip"/>
                                            ,
                                        </span>
                                    </t>
                                    <t t-if="company_city">
                                        <span t-esc="company_city"/>
                                        ,
                                    </t>
                                    <t t-if="company_state">
                                        <span t-esc="company_state"/>
                                    </t>
                                    <t t-if="company_country">
                                        <span t-esc="company_country"/>
                                    </t>
                                    <i class="fa fa-map-marker mx-2" aria-hidden="true"
                                       style="margin-left: 5px; border-radius: 50%; background-color: #ddd; padding: 5px;"/>
                                    <br/>
                                </td>
                            </tr>
                            <tr width="100%">
                                <td style="width: 100%;border-bottom: 1pt solid black;" colspan="2"/>
                            </tr>
                        </table>
                        <table width="100%" style="margin-top: 10px;margin-bottom: 5px;" class="table-borderless">
                            <tr>
                                <td style=" width: 100%; font-size: 14px;" align="right">
                                    <t t-if="company_vat">
                                        <span style="font-size: 14px; font-weight: bold;">TRN:</span>
                                        <span style="font-size: 14px; font-weight: bold; margin-left: 5px;"
                                              t-esc="company_vat"/>
                                    </t>
                                </td>
                            </tr>
                        </table>
                    </div>
                    <!-- <t t-foreach="values" t-as="vals" t-foreach-index="p_idx"> -->
