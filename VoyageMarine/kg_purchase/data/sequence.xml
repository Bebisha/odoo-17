<odoo>

    <data noupdate="1">
        <record id="purchase_seq_new" model="ir.sequence">
            <field name="name">Purchase Sequence</field>
            <field name="code">purchase.seq</field>
            <field name="prefix">PO%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="rfq_seq" model="ir.sequence">
            <field name="name">RFQ Sequence</field>
            <field name="code">rfq.seq</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="rfq_seq_new" model="ir.sequence">
            <field name="name">RFQ Sequence</field>
            <field name="code">rfq.seq.new</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="return_po_seq" model="ir.sequence">
            <field name="name">Return PO Sequence</field>
            <field name="code">return.po.seq</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>