        <record id="action_seq_lot_line" model="ir.actions.act_window">
            <field name="name">Sequence Lot</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sequence.lot.line</field>
            <field name="view_mode">tree</field>
            <field name="target">current</field>
        </record>

        <record id="sequence_lot_line_tree_view" model="ir.ui.view">
            <field name="name">kg.sequence.lot.tree.view</field>
            <field name="model">sequence.lot.line</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree decoration-success="is_already_created == True">
                    <field name="serial_no" string="SL.NO" force_save="1"/>
                    <field name="pro_id" force_save="1"/>
                    <field name="lot_number_text" readonly="True" string="Description" force_save="1"/>

                    <field name="product_temp_id" column_invisible="1" force_save="1"/>
                    <field name="product_category" column_invisible="1" force_save="1"/>
                    <field name="uom_id" column_invisible="1" force_save="1"/>
<!--                    <field name="is_created" column_invisible="1"/>-->
                    <field name="des" column_invisible="1" force_save="1"/>
                    <field name="lead_id" column_invisible="1" force_save="1"/>
                    <field name="make_id"  force_save="1"/>
                    <field name="model_id"  force_save="1"/>
                    <field name="name" string="Serial No/ID No"  force_save="1"/>
                    <field name="accesories" string="Accessories" force_save="1"/>
                    <field name="physical_status_id" string="Status" force_save="1"/>
                    <field name="remark" string="Remarks" force_save="1"/>
                    <!--                    <field name="lot_ids" column_invisible="1"/>-->
                    <!--                    <field name="serial_no" column_invisible="1"/>-->
                    <field name="qty" readonly="1" force_save="1"/>
                    <field name="unit_price" optional="hide" force_save="1"/>
                    <field name="is_already_created" optional="hide" force_save="1"/>
                    <field name="seq_line_lot" force_save="1"/>

<!--                    <button name="action_create_stock_quant" type="object" string="Update qty"-->
<!--                            class="btn-primary"/>-->
                </tree>
            </field>
        </record>
    </data>
</odoo>