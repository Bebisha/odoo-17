<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="paperformat_calibration_dimension_report" model="report.paperformat">
        <field name="name">Calibration Dimension Report</field>
        <field name="default" eval="True"/>
        <field name="format">A4</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">25</field>
        <field name="margin_bottom">36</field>
        <field name="margin_left">7</field>
        <field name="margin_right">7</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">25</field>
        <field name="disable_shrinking" eval="False"/>
        <field name="dpi">90</field>
    </record>

    <record id="action_calibration_dimension_report" model="ir.actions.report">
        <field name="name">Calibration Dimension Report</field>
        <field name="model">calibration.form</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">kg_jobsheet.report_calibration_dimension_product</field>
        <field name="report_file">kg_jobsheet.report_calibration_dimension_product</field>
        <field name="print_report_name">'Calibration Dimension - %s' % (object.name)</field>
        <field name="binding_model_id" ref="kg_jobsheet.model_calibration_form"/>
        <field name="binding_type">report</field>
        <field name="paperformat_id" ref="paperformat_calibration_dimension_report"/>
    </record>



    <record id="action_final_calibration_dimension_report" model="ir.actions.report">
        <field name="name">Final Calibration Dimension Report</field>
        <field name="model">repair.order</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">kg_jobsheet.report_final_calibration_dimension_product</field>
