<odoo>

    <record id="generate_employee_entries_wizard_form" model="ir.ui.view">
        <field name="name">generate.employee.entries.wizard.form</field>
        <field name="model">generate.employee.entries.wizard</field>
        <field name="priority" eval="8"/>
        <field name="arch" type="xml">
            <form string="Generate Employee Entries">
                <group>
                    <group>
                        <field name="start_date" required="1" readonly="1"/>
                        <field name="end_date" required="1" readonly="1"/>
                        <field name="entry_type" invisible="1"/>
                    </group>
                    <group>
                        <field name="company_id" required="1"/>
                        <field name="employee_ids" widget="many2many_tags"/>
                    </group>
                </group>
                <footer>
                    <button string="Generate" name="generate_employee_entries" type="object" class="btn-primary"/>
                    <button string="Cancel" name="cancel" type="object" special="cancel" class="btn-cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>