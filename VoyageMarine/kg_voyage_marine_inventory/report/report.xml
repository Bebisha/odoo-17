<odoo>
    <data>

        <record id="delivery_note_report_service" model="ir.actions.report">
            <field name="name">Delivery Note</field>
            <field name="model">stock.picking</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">kg_voyage_marine_inventory.kg_delivery_note_template</field>
            <field name="report_file">kg_voyage_marine_inventory.kg_delivery_note_template</field>
            <field name="binding_model_id" ref="model_stock_picking"/>
            <field name="print_report_name">'Delivery Note - %s' % object.name</field>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="kg_voyage_marine_inventory.paperformat_delivery_note"/>
        </record>

        <record id="kg_delivery_order_report" model="ir.actions.report">
            <field name="name">Delivery Order</field>
            <field name="model">stock.picking</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">kg_voyage_marine_inventory.kg_delivery_order_report_tmpl</field>
            <field name="report_file">kg_voyage_marine_inventory.kg_delivery_order_report_tmpl</field>
            <field name="binding_model_id" ref="kg_voyage_marine_inventory.model_stock_picking"/>
            <field name="print_report_name">'Delivery Order - %s' % object.name</field>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="kg_voyage_marine_inventory.paperformat_delivery_order"/>
        </record>

        <record id="stock.action_report_delivery" model="ir.actions.report">
            <field name="binding_model_id" eval="False"/>
        </record>

    </data>
</odoo>