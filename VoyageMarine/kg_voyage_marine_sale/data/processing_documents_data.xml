        </record>

        <record id="processing_doc_ac" model="processing.documents">
            <field name="name">Approval Certificate</field>
            <field name="is_default">False</field>
            <field name="description">Approval Certificate</field>
        </record>

        <record id="processing_doc_commercial_invoice" model="processing.documents">
            <field name="name">Commercial Invoice</field>
            <field name="is_default">False</field>
            <field name="description">Commercial invoice for customs clearance</field>
        </record>

        <record id="processing_doc_sr" model="processing.documents">
            <field name="name">Service Report</field>
            <field name="is_default">False</field>
            <field name="description">Service Report</field>
        </record>

        <record id="processing_doc_tr" model="processing.documents">
            <field name="name">Timesheet</field>
            <field name="is_default">False</field>
            <field name="description">Timesheet Report</field>
        </record>
