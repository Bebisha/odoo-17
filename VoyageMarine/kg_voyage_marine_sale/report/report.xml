<odoo>
    <data>

        <record id="action_quotation_report" model="ir.actions.report">
            <field name="name">Quote</field>
            <field name="model">sale.order</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">kg_voyage_marine_sale.report_quation_inherits</field>
            <field name="report_file">kg_voyage_marine_sale.report_quation_inherits</field>
            <field name="binding_model_id" ref="kg_voyage_marine_sale.model_sale_order"/>
            <field name="print_report_name">'Quotation - %s' % object.name</field>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="kg_voyage_marine_sale.paperformat_qtn_order"/>
        </record>

        <record id="action_so_report" model="ir.actions.report">
            <field name="name">Sale Order</field>
            <field name="model">sale.order</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">kg_voyage_marine_sale.report_so_inherits</field>
            <field name="report_file">kg_voyage_marine_sale.report_so_inherits</field>
            <field name="binding_model_id" ref="kg_voyage_marine_sale.model_sale_order"/>
            <field name="print_report_name">'Sale Order - %s' % object.name</field>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="kg_voyage_marine_sale.paperformat_qtn_order"/>
        </record>

        <record id="sale_pdf_quote_builder.action_report_saleorder_raw" model="ir.actions.report">
            <field name="name">Order Acknowledgement</field>
            <field name="paperformat_id" ref="kg_voyage_marine_sale.paperformat_qtn_order"/>
        </record>

        <record id="sale.action_report_saleorder" model="ir.actions.report">
            <field name="binding_model_id" eval="False"/>
        </record>

        <record id="action_service_report" model="ir.actions.report">
            <field name="name">Job / Service</field>
            <field name="model">sale.order</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">kg_voyage_marine_sale.service_report_template</field>
            <field name="report_file">kg_voyage_marine_sale.service_report_template</field>
            <field name="binding_model_id" ref="kg_voyage_marine_sale.model_sale_order"/>
            <field name="print_report_name">'Service Report - %s' % object.name</field>
            <field name="binding_type">report</field>
        </record>

        <record id="sale.action_report_pro_forma_invoice" model="ir.actions.report">
            <field name="paperformat_id" ref="kg_voyage_marine_sale.paperformat_proforma_invoice"/>
        </record>

        <record id="paperformat_commercial_print" model="report.paperformat">
            <field name="name">PO Print</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">40</field>
            <field name="margin_bottom">40</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">39</field>
            <field name="disable_shrinking" eval="False"/>
            <field name="dpi">90</field>
        </record>

        <record id="standard_pdf_commercial_invoice_report" model="ir.actions.report">
            <field name="name">Commercial Invoice</field>
            <field name="model">package.list</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">kg_voyage_marine_sale.voyage_commercial_invoice_report</field>
            <field name="report_file">kg_voyage_marine_sale.voyage_commercial_invoice_report</field>
            <field name="print_report_name">('Commercial Invoice')</field>
            <field name="binding_model_id" ref="kg_voyage_marine_sale.model_package_list"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="paperformat_commercial_print"/>
        </record>

        <record id="standard_pdf_voyage_packing_list_report" model="ir.actions.report">
            <field name="name">Packing List</field>
            <field name="model">package.list</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">kg_voyage_marine_sale.voyage_packing_list_report</field>
            <field name="report_file">kg_voyage_marine_sale.voyage_packing_list_report</field>
            <field name="print_report_name">('Packing list')</field>
            <field name="binding_model_id" ref="kg_voyage_marine_sale.model_package_list"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="paperformat_commercial_print"/>
        </record>
        <record id="action_cipl" model="ir.actions.report">
            <field name="name">CIPL</field>
            <field name="model">package.list</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">kg_voyage_marine_sale.cipl_report_template</field>
            <field name="report_file">kg_voyage_marine_sale.cipl_report_template</field>
            <field name="print_report_name">('CIPL')</field>
            <field name="binding_model_id" ref="kg_voyage_marine_sale.model_package_list"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="paperformat_commercial_print"/>
        </record>


    </data>
</odoo>