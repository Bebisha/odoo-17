<odoo>
    <data noupdate="1">
        <record id="seq_crm_estimation" model="ir.sequence">
            <field name="name">Estimation</field>
            <field name="code">kg.crm.estimation</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_inspection_req" model="ir.sequence">
            <field name="name">Inspection</field>
            <field name="code">inspection.seq</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_enq_new" model="ir.sequence">
            <field name="name">Enquiry</field>
            <field name="code">enq.seq</field>
            <field name="prefix">ENQ%(y)s%(month)s</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="jr_enq" model="ir.sequence">
            <field name="name">Job costing</field>
            <field name="code">jr.seq</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_serial_number" model="ir.sequence">
            <field name="name">Serial Number</field>
            <field name="code">stock.serial.number</field>
            <field name="prefix">SERIAL-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

         <record id="seq_inspection_report" model="ir.sequence">
            <field name="name">Inspection Report</field>
            <field name="code">inspection.report.seq</field>
            <field name="prefix">VG_IMS_F</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>