<odoo>
    <data>
        <record id="product_template_only_form_view_alfahad" model="ir.ui.view">
            <field name="name">product.template.form.alfahad</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='categ_id']" position="after">
                    <field name="is_published"/>
                </xpath>
                <xpath expr="//group[@name='group_lots_and_weight']" position="after">
                    <group name="warehouse" string="WAREHOUSE">
                        <field name="warehouse_ids" widget="many2many_tags"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>