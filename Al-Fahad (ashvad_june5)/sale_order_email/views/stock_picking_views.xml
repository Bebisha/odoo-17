<?xml version="1.0"?>
<odoo>
    <data>
        <record id="stock_picking_inherit" model="ir.ui.view">
            <field name="name">stock.picking.form.inherited</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <field name="location_id" position="after">
                    <field name="do_num" invisible="picking_type_code != 'outgoing'"/>
                    <field name="receipt_num" invisible="picking_type_code != 'incoming'"/>
                </field>
            </field>
        </record>

        <record id="account_move_inherit_fahad" model="ir.ui.view">
            <field name="name">account.move.form.inherited</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='journal_id']" position="after">
                    <!--                <field name="partner_shipping_id" position="after">-->
                    <field name="invoice_num"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
