<odoo>
    <record id="import_license_wizard_form" model="ir.ui.view">
        <field name="name">import.license.date.wizard</field>
        <field name="model">import.license.date.wizard</field>
        <field name="arch" type="xml">
            <form string="License Date template">
                <sheet>
                    <group>

                        <field name="data_file"/>
                    </group>
                    <footer>
                        <button name="update_file" string="Update" type="object"
                                class="oe_highlight"/>
                        <button string="Close" class="btn-secondary" special="cancel"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>

    <record id="import_license_date_wizard_wizard" model="ir.actions.act_window">
        <field name="name">License Date Import</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">import.license.date.wizard</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="import_license_wizard_form"/>
        <field name="target">new</field>
    </record>
    <menuitem id="import_license_date_wizard_template" name="Import License Date " parent="registration_menu_root"
              action="import_license_date_wizard_wizard" sequence="40"/>

</odoo>