<odoo>
    <record id="daily_crm_notification" model="ir.cron">
        <field name="name">Daily Crm Leads Notifications</field>
        <field name="model_id" ref="crm.model_crm_lead"/>
        <field name="state">code</field>
        <field name="code">model.action_send_daily_lead_report()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
    </record>
</odoo>