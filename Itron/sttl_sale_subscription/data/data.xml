<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="ir_contract_reference_free_support" model="ir.sequence">
           <field name="name">Sequence Numbers</field>
           <field name="code">contract.reference.free.support</field>
           <field name="prefix">CR</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>

       <record id="seq_contract_number_free_support" model="ir.sequence">
            <field name="name">Free Support Contract Number</field>
            <field name="code">contract.number.free.support</field>
            <field name="prefix">FREE/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>