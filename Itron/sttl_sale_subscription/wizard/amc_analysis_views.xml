<odoo>
    <data>

        <record id="amc_analysis_wizard_view_form" model="ir.ui.view">
            <field name="name">amc.analysis.wizard.view.form</field>
            <field name="model">amc.analysis</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="project_id" options="{'no_create': True, 'no_open': True}"/>
                                <field name="start_date" readonly="1" force_save="1"/>
                            </group>
                            <group>
                                <field name="contract_id" options="{'no_create': True, 'no_open': True}"/>
                                <field name="end_date" readonly="1" force_save="1"/>
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button name="action_print_xlsx" class="oe_right oe_highlight"  type="object" string="XLSX"/>
<!--                        <button name="action_print_pdf" class="oe_right oe_highlight"  type="object" string="PDF"/>-->
                        <button string="Close" class="btn-secondary" special="cancel" data-hotkey="z"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_amc_analysis" model="ir.actions.act_window">
            <field name="name">AMC Analysis</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">amc.analysis</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

        <menuitem
                id="menu_amc_analysis"
                action="action_amc_analysis"
                name="AMC Analysis"
                parent="project.menu_project_report"
                sequence="5"/>


    </data>
</odoo>