<odoo>
    <data noupdate="1">
        <record model="ir.module.category" id="module_user_access">
            <field name="name">User Access</field>
            <field name="sequence">103</field>
        </record>
        <record id="group_discuss_access" model="res.groups">
            <field name="name">Discuss</field>
            <field name="category_id" ref="module_user_access"/>
        </record>
        <record id="group_contacts_access" model="res.groups">
            <field name="name">Contacts</field>
            <field name="category_id" ref="module_user_access"/>
        </record>
        <record id="group_to_do_access" model="res.groups">
            <field name="name">To Do</field>
            <field name="category_id" ref="module_user_access"/>
        </record>
        <record id="group_calendar_access" model="res.groups">
            <field name="name">Calendar</field>
            <field name="category_id" ref="module_user_access"/>
        </record>
        <record id="group_link_tracker_access" model="res.groups">
            <field name="name">Link Tracker</field>
            <field name="category_id" ref="module_user_access"/>
        </record>
        <record id="group_apps_access" model="res.groups">
            <field name="name">Apps</field>
            <field name="category_id" ref="module_user_access"/>
        </record>
        <record id="base.user_admin" model="res.users">
            <field name="groups_id" eval="[(4, ref('kg_parent_menu_user_access.group_discuss_access')),
                                           (4, ref('kg_parent_menu_user_access.group_contacts_access')),
                                           (4, ref('kg_parent_menu_user_access.group_to_do_access')),
                                           (4, ref('kg_parent_menu_user_access.group_calendar_access')),
                                           (4, ref('kg_parent_menu_user_access.group_link_tracker_access')),
                                           (4, ref('kg_parent_menu_user_access.group_apps_access'))]"/>
        </record>
    </data>
</odoo>
