<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="material_request_dashboard_action" model="ir.actions.client">
        <field name="name">Material Request Dashboard</field>
        <field name="tag">material_dashboard_tag</field>
    </record>

    <record id="inventory_valuation_dashboard_action" model="ir.actions.client">
        <field name="name">Stock Analysis</field>
        <field name="tag">valuation_dashboard_tag</field>
    </record>
    <menuitem id="kg_inventory_dashboard"
              name="Inventory"
              groups="stock.group_stock_user"
              parent="kg_timeoff_dashboard.root_menu_dashboard_kg"/>

    <menuitem id="kg_material_valuation_dashboard"
              name="Stock Analysis"
              action="inventory_valuation_dashboard_action"
              parent="kg_material_request.kg_inventory_dashboard"
              sequence="2"
    />
    <menuitem id="kg_material_waiting_dashboard"
              name="MR Approval"
              action="material_request_dashboard_action"
              parent="kg_material_request.kg_inventory_dashboard"
              sequence="1"
    />

