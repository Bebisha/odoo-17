<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form_daily_report" model="ir.ui.view">
            <field name="name">res.config.settings.view.daily.report.form.inherit</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="base.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//app[@name='crm']" position="inside">
                    <block title="Daily Report">
                        <div class="row mt16 o_settings_container" id="end_contract_setting">
                            <div class="col-12 col-lg-6 o_setting_box">
                                <group>
                                    <field name="daily_report_manager_ids" widget="many2many_tags"/>
                                </group>
                            </div>
                        </div>
                    </block>
                </xpath>
            </field>
        </record>

    </data>
</odoo>