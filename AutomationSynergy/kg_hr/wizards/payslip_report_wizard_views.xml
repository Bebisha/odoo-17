<odoo>
    <data>
        <record id="action_payslip_wizard" model="ir.actions.act_window">
            <field name="name">Salary Statement File</field>
            <field name="target">new</field>
            <field name="res_model">payslip.report.wizard</field>
            <field name="view_mode">form</field>
        </record>

        <record id="report_wizard_form" model="ir.ui.view">
            <field name="name">payslip.report.wizard.form</field>
            <field name="model">payslip.report.wizard</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Salary Statement File">
                    <group>

                        <group>
                            <field name="date_from" required="1"/>
                            <field name="actual_deduction" invisible="1"/>
                        </group>
                        <group>
                            <field name="date_to" required="1"/>
                            <field name="expact_wiz"  invisible="1"/>
                        </group>
                        <group>
                            <field name="fileout" invisible="1"/>
                            <field name="fileout_filename" invisible="1"/>
                        </group>
                    </group>
                    <footer>
                        <button string="Print xlsx" name="print_sif_xlsx" type="object" class="btn-primary"/>
                        <button string="Cancel" name="cancel" type="object" special="cancel" class="btn-cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>