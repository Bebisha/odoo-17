<odoo>
    <record id="product_model_tree_view" model="ir.ui.view">
        <field name="name">product.model.tree.view</field>
        <field name="model">product.model</field>
        <field name="arch" type="xml">
            <tree string="model" editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_product_model" model="ir.actions.act_window">
        <field name="name">Product Model</field>
        <field name="res_model">product.model</field>
        <field name="view_mode">tree</field>
    </record>
    <menuitem id="menu_product_model" name="Product Model"
              parent="stock.menu_product_in_config_stock"
              action="action_product_model"/>
</odoo>